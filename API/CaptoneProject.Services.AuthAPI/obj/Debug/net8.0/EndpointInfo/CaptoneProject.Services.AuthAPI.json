                  },
                  "Certificate": {
                    "type": "string",
                    "format": "binary"
                  },
                  "Resume": {
                    "type": "string",
                    "format": "binary"
                  }
                }
              },
              "encoding": {
                "UserName": {
                  "style": "form"
                },
                "Email": {
                  "style": "form"
                },
                "Password": {
                  "style": "form"
                },
                "Certificate": {
                  "style": "form"
                },
